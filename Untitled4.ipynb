    "\n",
    "def fetch_data():\n",
    "    strike_old = []\n",
    "    volume_old = []\n",
    "    count = 0\n",
    "    time_gap = 10\n",
    "    while True:\n",
    "        count = count+1\n",
    "        response = requests.get(option_url, headers=headers)\n",
    "        soup = BeautifulSoup(response.text, 'html.parser')\n",
    "        response_text = response.text\n",
    "        expiry_date = '09-May-2024'\n",
    "        json_object = json.loads(response_text)\n",
    "        data = json_object['records']['data']\n",
    "        e_data = json_object['records']['expiryDates']\n",
    "        oc_data = {}\n",
    "        for ed in e_data:\n",
    "            oc_data[ed]={\"CE\":[], \"PE\":[]}\n",
    "            for di in range(len(data)):\n",
    "                \n",
    "                if data[di]['expiryDate'] == ed:\n",
    "                    if 'CE' in data[di].keys() and data[di]['CE']['expiryDate'] == ed:\n",
    "                        oc_data[ed][\"CE\"].append(data[di]['CE'])\n",
    "                    else:\n",
    "                        oc_data[ed][\"CE\"].append('-')\n",
    "        \n",
    "                    if 'PE' in data[di].keys() and data[di]['PE']['expiryDate'] == ed:\n",
    "                        oc_data[ed][\"PE\"].append(data[di]['PE'])\n",
    "                        \n",
    "                    else:\n",
    "                        oc_data[ed][\"PE\"].append('-')\n",
    "                        \n",
    "        oc_data_dt=oc_data[expiry_date]\n",
    "        CE=list(oc_data_dt['CE'])\n",
    "        PE=list(oc_data_dt['PE'])\n",
    "        strike = []\n",
    "        volume = []\n",
    "        for a in PE:\n",
    "            if a != '-':\n",
    "                strike.append(a['strikePrice'])\n",
    "                volume.append(a['totalTradedVolume'])\n",
    "        if set(volume) != set(volume_old):\n",
    "            print(\"lists changed\", count*time_gap, \"seconds gap\")\n",
    "            count = 0\n",
    "        # else:\n",
    "        #     print(\"SAME LISTS!!\")\n",
    "            # print(\"Totalvolume:\",volume)\n",
    "        \n",
    "        strike_old = strike[:]\n",
    "        volume_old = volume[:]\n",
    "    \n",
    "        time.sleep(time_gap)\n",
    "while True:\n",
    "    try:\n",
    "        fetch_data()\n",
    "    except:\n",
    "        print(\"\\n JSONDecodeError \\n\\n\")\n",
    "        \n",
